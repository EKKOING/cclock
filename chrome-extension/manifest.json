{
  "manifest_version": 2,

  "name": "Clock Plugin",
  "description": "This loads a school clock.",
  "version": "0.1",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "index.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ],
  "content_security_policy": "script-src 'self' https://cdnjs.cloudflare.com https://fonts.googleapis.com https://darksky.net; object-src 'self'"
}
